c; \n \nnamespace MEZBELE.Controllers \n{ \n    /// <summary> \n    /// Rapor sayfasının kontrolcüsü. \n    /// </summary> \n    public class RaporController : Controller \n    { \n        /// <summary> \n        /// Veritabanı. \n        /// </summary> \n        private readonly MezbeleDBEntities db = new MezbeleDBEntities(); \n \n        /// <summary> \n        /// ViewModel. \n        /// </summary> \n        private VM vm; \n \n        /// <summary> \n        /// Rapor sayfası. \n        /// </summary> \n        /// <param name=\"projeKimligi\">Raporu oluşturulacak projenin kimliği</param> \n        /// <returns>Rapor sayfasına yönlendirir</returns> \n        public ActionResult Index(int projeKimligi) \n        { \n            if (Request.Cookies[\"KullaniciKimligi\"] != null) \n            { \n                vm = new VM(); \n \n                vm.AktifProje = (from p in db.Proje \n                                 where p.ID == projeKimligi \n                                 select p).SingleOrDefault(); \n \n                vm.Yonetici = (from y in db.Kullanici \n                               where y.ID == vm.AktifProje.YoneticiID \n                               select y).SingleOrDefault(); \n \n                vm.Musteri = (from m in db.Kullanici \n                              where m.ID == vm.AktifProje.ProjeSahibiID \n                              select m).SingleOrDefault(); \n \n                return View(vm); \n            } \n            return RedirectToAction(\"Index\", \"Landing\"); \n        } \n    } \n}"}],"summary":"<p>Rapor sayfasının kontrolcüsü.</p>","fields":[{"id":"field.MEZBELE.Controllers.RaporController.db","urlId":"fieldMEZBELEControllersRaporControllerdb","type":"field","accessibility":"private","name":"db","region":{"start":"16","end":"16","filename":"RaporController.cs"},"summary":"<p>Veritabanı.</p>","syntax":"private readonly MezbeleDBEntities db","linkedSyntax":"private readonly [MezbeleDBEntities](#/type/MEZBELE.Models.Mezbele