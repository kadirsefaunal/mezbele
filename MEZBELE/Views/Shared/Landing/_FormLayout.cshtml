<!DOCTYPE html>
<html lang="tr">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Mezbele</title>

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
